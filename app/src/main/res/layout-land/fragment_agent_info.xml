<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".Screens.Fragment.AgentInfoFragment">

<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">
    <androidx.cardview.widget.CardView

        android:id="@+id/PersnolInformation"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

android:layout_margin="@dimen/_5sdp"



        app:cardCornerRadius="@dimen/_20sdp"

        >


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:padding="@dimen/_8sdp">

            <com.denzcoskun.imageslider.ImageSlider
                android:id="@+id/imageSlider"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/_110sdp"
                app:strokeColor="@color/nearGreen"
                app:strokeWidth="@dimen/_1sdp"
                app:shapeAppearanceOverlay="@style/roundedImageView"

                app:iss_auto_cycle="true"
                app:iss_period="1000"
                app:iss_delay="1000"/>


         <ScrollView
             android:layout_width="match_parent"
             android:layout_height="wrap_content">
             <LinearLayout
                 android:layout_width="match_parent"
                 android:layout_height="wrap_content"
                 android:layout_gravity="start"
                 android:gravity="center"
                 android:orientation="vertical"
                 android:padding="@dimen/_10sdp">

                 <LinearLayout
                     android:layout_width="match_parent"
                     android:layout_height="wrap_content"

                     android:layout_gravity="right"
                     android:gravity="right"
                     android:orientation="horizontal">

                     <TextView
                         android:id="@+id/ClinetNam"
                         android:layout_width="wrap_content"
                         android:layout_height="wrap_content"
                         android:text="سيد احمد السيد عوضين"
                         android:textColor="@color/nearGreen"
                         android:textSize="@dimen/_12ssp" />

                     <TextView
                         android:layout_width="wrap_content"
                         android:layout_height="wrap_content"
                         android:text="الاسم: "
                         android:textColor="#000000"
                         android:textSize="@dimen/_12ssp"
                         android:textStyle="bold" />
                 </LinearLayout>
                 <LinearLayout
                     android:layout_width="match_parent"
                     android:layout_height="wrap_content"

                     android:layout_gravity="right"
                     android:gravity="right"
                     android:orientation="horizontal">

                     <TextView
                         android:id="@+id/NickName"
                         android:layout_width="wrap_content"
                         android:layout_height="wrap_content"
                         android:text="سيد احمد السيد عوضين"
                         android:textColor="@color/nearGreen"
                         android:textSize="@dimen/_12ssp" />

                     <TextView
                         android:layout_width="wrap_content"
                         android:layout_height="wrap_content"
                         android:text="اللقب : "
                         android:textColor="#000000"
                         android:textSize="@dimen/_12ssp"
                         android:textStyle="bold" />
                 </LinearLayout>
                 <LinearLayout
                     android:layout_width="match_parent"
                     android:layout_height="wrap_content"
                     android:layout_gravity="right"
                     android:gravity="right"
                     android:orientation="horizontal">

                     <TextView
                         android:id="@+id/Adders"
                         android:layout_width="wrap_content"
                         android:layout_height="wrap_content"
                         android:text="31 ش 66 مدينة نصر"
                         android:textColor="@color/nearGreen"
                         android:textSize="@dimen/_12ssp" />

                     <TextView
                         android:layout_width="wrap_content"
                         android:layout_height="wrap_content"

                         android:text="العنوان: "
                         android:textColor="#000000"
                         android:textSize="@dimen/_12ssp"
                         android:textStyle="bold" />
                 </LinearLayout>

                 <LinearLayout
                     android:layout_width="match_parent"
                     android:layout_height="wrap_content"
                     android:layout_gravity="right"
                     android:gravity="right"
                     android:orientation="horizontal">

                     <TextView
                         android:id="@+id/AddersCard"
                         android:layout_width="wrap_content"
                         android:layout_height="wrap_content"
                         android:text="31 ش 66 مدينة نصر"
                         android:textColor="@color/nearGreen"
                         android:textSize="@dimen/_12ssp" />

                     <TextView
                         android:layout_width="wrap_content"
                         android:layout_height="wrap_content"

                         android:text="العنوان في البطاقة :  "
                         android:textColor="#000000"
                         android:textSize="@dimen/_12ssp"
                         android:textStyle="bold" />
                 </LinearLayout>
                 <LinearLayout
                     android:layout_width="match_parent"
                     android:layout_height="wrap_content"
                     android:layout_gravity="right"
                     android:gravity="right"

                     android:orientation="horizontal">

                     <TextView
                         android:id="@+id/CardNumber"
                         android:layout_width="wrap_content"
                         android:layout_height="wrap_content"

                         android:text="01220000000"
                         android:textColor="@color/nearGreen"
                         android:textSize="@dimen/_12ssp" />

                     <TextView
                         android:layout_width="wrap_content"
                         android:layout_height="wrap_content"
                         android:text="الرقم القومي : "
                         android:textColor="#000000"
                         android:textSize="@dimen/_12ssp"
                         android:textStyle="bold" />
                 </LinearLayout>
                 <LinearLayout
                     android:layout_width="match_parent"
                     android:layout_height="wrap_content"
                     android:layout_gravity="right"
                     android:gravity="right"

                     android:orientation="horizontal">

                     <TextView
                         android:id="@+id/PhonNumber"
                         android:layout_width="wrap_content"
                         android:layout_height="wrap_content"

                         android:text="01220000000"
                         android:textColor="@color/nearGreen"
                         android:textSize="@dimen/_12ssp" />

                     <TextView
                         android:layout_width="wrap_content"
                         android:layout_height="wrap_content"
                         android:text="رقم الموبيل: "
                         android:textColor="#000000"
                         android:textSize="@dimen/_12ssp"
                         android:textStyle="bold" />
                 </LinearLayout>
                 <LinearLayout
                     android:layout_width="match_parent"
                     android:layout_height="wrap_content"
                     android:layout_gravity="right"
                     android:gravity="right"

                     android:orientation="horizontal">

                     <TextView
                         android:id="@+id/PhonNumber2"
                         android:layout_width="wrap_content"
                         android:layout_height="wrap_content"

                         android:text="01220000000"
                         android:textColor="@color/nearGreen"
                         android:textSize="@dimen/_12ssp" />

                     <TextView
                         android:layout_width="wrap_content"
                         android:layout_height="wrap_content"
                         android:text="رقم الموبيل 2 : "
                         android:textColor="#000000"
                         android:textSize="@dimen/_12ssp"
                         android:textStyle="bold" />
                 </LinearLayout>
                 <LinearLayout
                     android:layout_width="match_parent"
                     android:layout_height="wrap_content"
                     android:layout_gravity="right"
                     android:gravity="right"

                     android:orientation="horizontal">

                     <TextView
                         android:id="@+id/PhonNumber3"
                         android:layout_width="wrap_content"
                         android:layout_height="wrap_content"

                         android:text="01220000000"
                         android:textColor="@color/nearGreen"
                         android:textSize="@dimen/_12ssp" />

                     <TextView
                         android:layout_width="wrap_content"
                         android:layout_height="wrap_content"
                         android:text="الرقم الارضي : "
                         android:textColor="#000000"
                         android:textSize="@dimen/_12ssp"
                         android:textStyle="bold" />
                 </LinearLayout>
                 <!--
                   <LinearLayout
                       android:layout_width="match_parent"
                       android:layout_height="wrap_content"

                       android:layout_gravity="right"
                       android:gravity="right"
                       android:orientation="horizontal">

                       <TextView
                           android:id="@+id/Gander"
                           android:layout_width="wrap_content"
                           android:layout_height="wrap_content"
                           android:text="ذكر"
                           android:textColor="@color/nearGreen"
                           android:textSize="@dimen/_12ssp" />

                       <TextView
                           android:layout_width="wrap_content"
                           android:layout_height="wrap_content"
                           android:text="النوع : "
                           android:textColor="#000000"
                           android:textSize="@dimen/_12ssp"
                           android:textStyle="bold" />
                   </LinearLayout>
                   -->
                 <LinearLayout
                     android:layout_width="match_parent"
                     android:layout_height="wrap_content"
                     android:layout_gravity="right"
                     android:gravity="right"

                     android:orientation="horizontal">

                     <TextView
                         android:id="@+id/Job"
                         android:layout_width="wrap_content"
                         android:layout_height="wrap_content"

                         android:text="مونتير"
                         android:textColor="@color/nearGreen"
                         android:textSize="@dimen/_12ssp" />

                     <TextView
                         android:layout_width="wrap_content"
                         android:layout_height="wrap_content"
                         android:text="الوظفية : "
                         android:textColor="#000000"
                         android:textSize="@dimen/_12ssp"
                         android:textStyle="bold" />
                 </LinearLayout>
                 <LinearLayout
                     android:layout_width="match_parent"
                     android:layout_height="wrap_content"
                     android:layout_gravity="right"
                     android:gravity="right"

                     android:orientation="horizontal">

                     <TextView
                         android:id="@+id/JobCard"
                         android:layout_width="wrap_content"
                         android:layout_height="wrap_content"

                         android:text="مونتير"
                         android:textColor="@color/nearGreen"
                         android:textSize="@dimen/_12ssp" />

                     <TextView
                         android:layout_width="wrap_content"
                         android:layout_height="wrap_content"
                         android:text="الوظفية في البطاقة: "
                         android:textColor="#000000"
                         android:textSize="@dimen/_12ssp"
                         android:textStyle="bold" />
                 </LinearLayout>
                 <TextView
                     android:id="@+id/DateTaske"
                     android:layout_width="match_parent"
                     android:layout_height="wrap_content"
                     android:gravity="left"
                     android:text="06/07/2022"
                     android:textSize="@dimen/_10ssp" />
             </LinearLayout>

         </ScrollView>


        </LinearLayout>
    </androidx.cardview.widget.CardView>
</LinearLayout>

</FrameLayout>